{
  "name": "ai-resume-analyzer",
  "version": "1.0.0",
  "description": "AI-Powered Resume & Skill Analyzer using MERN stack with Gemini AI integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "server:install": "cd server && npm install",
    "client:install": "cd client && npm install",
    "install:all": "npm run server:install && npm run client:install",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "resume",
    "ai",
    "analysis",
    "gemini",
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "career",
    "job-search"
  ],
  "author": "AI Resume Analyzer Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-resume-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-resume-analyzer/issues"
  },
  "homepage": "https://github.com/your-username/ai-resume-analyzer#readme"
}
